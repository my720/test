
public class Person {
	//フィールド
	public String name;
	public int age;
	//コンストラクタ
	public Person(String name,int age){
		this.name=name;
		this.age=age;
	}
	public Person(){

	}
	//getter、setter
	public String getName(){
		return name;
	}
	public void setName(String name){
		this.name=name;
	}
	public int getAge(){
		return age;
	}
	public void setAge(int age){
		this.age=age;
	}
}